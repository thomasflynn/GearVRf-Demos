        <Script DEF='OverScript01'>
        <field accessType="inputOnly" type="SFBool" name="isOver"/>
        <field accessType="outputOnly" type="SFColor" name="diffuseColor_changed"/>
        <![CDATA[
              
        ecmascript:

        function isOver(val) 
        { 
            if (val == true) 
            { 
                diffuseColor_changed = new SFColor(.8, .8, .8); 
            } 
            else
            { 
                diffuseColor_changed = new SFColor(.7, .7, .7); 
            }
        }
            
        ]]>
        </Script> 
        <ROUTE toField="isOver" toNode="OverScript01" fromField="isOver" fromNode="TouchSensor01-SENSOR"/>
        <ROUTE toField="diffuseColor" toNode="video_sphere_01_mat" fromField="diffuseColor_changed" fromNode="OverScript01"/>


        <Script DEF='ActiveScript01'>
        <field accessType="inputOnly" type="SFBool" name="isActive"/>
        <field accessType="outputOnly" type="SFColor" name="diffuseColor_changed"/>
        <field name='new_url' type='MFString' accessType='inputOutput'/> 
        <![CDATA[
              
        ecmascript:

        function isActive(val) 
        { 
            if (val == true) 
            { 
                diffuseColor_changed = new SFColor(.9, .9, .9); 
				new_url = new MFString("Gear360/HaleakalaSunset.astc");
            } 
            else
            { 
                diffuseColor_changed = new SFColor(.8, .8, .8); 
            }
        }
            
        ]]>
        </Script> 
        <ROUTE toField="isActive" toNode="ActiveScript01" fromField="isActive" fromNode="TouchSensor01-SENSOR"/>
